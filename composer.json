{
  "name": "simplygoodwork/marketo-api",
  "description": "Pass form submissions to Marketo",
  "type": "craft-plugin",
  "version": "1.1.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "marketo api"
  ],
  "support": {
    "docs": "https://github.com/simplygoodwork/marketo-api/blob/master/README.md",
    "issues": "https://github.com/simplygoodwork/marketo-api/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "simplygoodwork",
      "homepage": "http://simplygoodwork.com"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0-RC1"
  },
  "autoload": {
    "psr-4": {
      "simplygoodwork\\marketoapi\\": "src/"
    }
  },
  "extra": {
    "name": "Marketo API",
    "handle": "marketo-api",
    "hasCpSettings": true,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/simplygoodwork/marketo-api/master/CHANGELOG.md",
    "components": {
      "marketoApiService": "simplygoodwork\\marketoapi\\services\\MarketoApiService"
    },
    "class": "simplygoodwork\\marketoapi\\MarketoApi"
  }
}
